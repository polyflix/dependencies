{
  "tagFormat": "${process.env.PACKAGE_NAME}-v${version}",
  "commitPaths": "npm-packages//${process.env.PACKAGE_NAME}/*",
  "branches": ["main"],
  "plugins": [
    [
      "@semantic-release/git",
      {
        "assets": ["package.json"],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ]
  ]
}
